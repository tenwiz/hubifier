{
  "name": "hubifier",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf build",
    "serve": "parcel src/background.ts src/popup/index.html -d build",
    "build": "yarn run clean && parcel build src/background.ts src/popup/index.html -d build",
    "watch": "parcel watch build src/background.ts src/popup/index.html -d build"
  },
  "devDependencies": {
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/chrome": "^0.0.89",
    "@types/moment": "^2.13.0",
    "axios": "^0.21.2",
    "eslint": "^6.5.1",
    "github": "^14.0.0",
    "moment": "^2.24.0",
    "preact": "^10.0.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
